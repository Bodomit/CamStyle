    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Train Market (c1-c2)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/CycleGAN-for-CamStyle/train.py",
            "args": [
                "--dataroot", "~/datasets/market",
                "--name", "market-c1-c2",
                "--checkpoints_dir", "~/results/camstyle_market_cyclegans/",
                "--camA", "1",
                "--camB", "2"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Train MMF (A-B)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/CycleGAN-for-CamStyle/train.py",
            "args": [
                "--dataroot", "~/datasets/mmf",
                "--name", "mmf-A-B",
                "--checkpoints_dir", "~/results/camstyle_mmf_cyclegans/",
                "--dataset_mode", "unaligned",
                "--phase", ""
            ],
            "console": "integratedTerminal"
        }
    ]
